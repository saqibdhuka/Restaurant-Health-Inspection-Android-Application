<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/inspectionArrayList_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_constraintStart_toEndOf="@id/inspectionArrayList_ver02_guideline"
    app:layout_constraintTop_toBottomOf="@id/inspectionArrayList_hor02_guideline"
    app:layout_constraintEnd_toStartOf="@id/inspectionArrayList_ver98_guideline">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/inspectionArrayList_ver02_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.02" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/inspectionArrayList_ver63_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.63" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/inspectionArrayList_ver98_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.98" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/inspectionArrayList_hor02_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.02" />



    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/inspectionArrayList_hor08_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.1" />



    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/inspectionArrayList_hor04_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.06" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/inspectionArrayList_hor07_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/inspectionArrayList_ver30_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.3" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/inspectionArrayList_ver44_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.44" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/inspectionArrayList_ver80_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.8" />


    <TextView
        android:id="@+id/inspectionArrayList_inspection_hazard"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:text="@string/inspectionListView_hazard"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toStartOf="@+id/inspectionArrayList_ver30_guideline"
        app:layout_constraintStart_toEndOf="@id/inspectionArrayList_ver02_guideline"
        app:layout_constraintTop_toBottomOf="@id/inspectionArrayList_hor02_guideline"
        app:layout_constraintBottom_toTopOf="@id/inspectionArrayList_hor04_guideline"/>

    <TextView
        android:id="@+id/inspectionArrayList_inspection_hazard_lv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:maxLines="1"
        android:text="@string/sample_hazard_lv"
        android:gravity="center"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toStartOf="@+id/inspectionArrayList_ver30_guideline"
        app:layout_constraintStart_toStartOf="@id/inspectionArrayList_ver02_guideline"
        app:layout_constraintTop_toBottomOf="@+id/inspectionArrayList_inspection_hazard"
        app:layout_constraintBottom_toTopOf="@id/inspectionArrayList_hor08_guideline"/>

    <View
        android:id="@+id/inspectionArrayList_inspection_hazard_color"
        android:layout_width="0dp"
        android:layout_height="15dp"
        android:background="@color/colorGreen"
        android:visibility="visible"
        app:layout_constraintEnd_toStartOf="@+id/inspectionArrayList_ver30_guideline"
        app:layout_constraintStart_toEndOf="@id/inspectionArrayList_ver02_guideline"
        app:layout_constraintTop_toBottomOf="@+id/inspectionArrayList_inspection_hazard_lv"
        />

    <TextView
        android:id="@+id/inspectionArrayList_inspection_crit_text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:text="@string/inspectionListView_crit"
        android:textAlignment="textEnd"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toStartOf="@+id/inspectionArrayList_ver80_guideline"
        app:layout_constraintStart_toEndOf="@+id/inspectionArrayList_ver44_guideline"
        app:layout_constraintTop_toBottomOf="@id/inspectionArrayList_hor02_guideline"
        app:layout_constraintBottom_toTopOf="@id/inspectionArrayList_hor04_guideline"/>

    <TextView
        android:id="@+id/inspectionArrayList_inspection_crit"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:text="@string/sample_crit_num"
        android:gravity="center"
        app:autoSizeTextType="uniform"
        app:layout_constraintStart_toEndOf="@+id/inspectionArrayList_ver80_guideline"
        app:layout_constraintTop_toBottomOf="@id/inspectionArrayList_hor02_guideline"
        app:layout_constraintBottom_toTopOf="@id/inspectionArrayList_hor04_guideline"
        app:layout_constraintEnd_toStartOf="@id/inspectionArrayList_ver98_guideline"/>

    <TextView
        android:id="@+id/inspectionArrayList_inspection_noncrit_text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:text="@string/inspectionListView_noncrit"
        app:autoSizeTextType="uniform"
        android:textAlignment="textEnd"
        app:layout_constraintEnd_toStartOf="@+id/inspectionArrayList_ver80_guideline"
        app:layout_constraintStart_toEndOf="@+id/inspectionArrayList_ver44_guideline"
        app:layout_constraintTop_toBottomOf="@+id/inspectionArrayList_inspection_crit_text"
        app:layout_constraintBottom_toTopOf="@id/inspectionArrayList_hor07_guideline"/>

    <TextView
        android:id="@+id/inspectionArrayList_inspection_noncrit"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:text="@string/sample_noncrit_num"
        android:gravity="center"
        app:autoSizeTextType="uniform"
        app:layout_constraintStart_toEndOf="@+id/inspectionArrayList_ver80_guideline"
        app:layout_constraintBottom_toTopOf="@id/inspectionArrayList_hor07_guideline"
        app:layout_constraintEnd_toStartOf="@id/inspectionArrayList_ver98_guideline"
        app:layout_constraintTop_toBottomOf="@+id/inspectionArrayList_inspection_crit" />

    <TextView
        android:id="@+id/inspectionArrayList_inspection_date_text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:text="@string/inspectionListView_date"
        app:autoSizeTextType="uniform"
        android:textAlignment="textEnd"
        app:layout_constraintEnd_toStartOf="@+id/inspectionArrayList_ver44_guideline"
        app:layout_constraintStart_toStartOf="@+id/inspectionArrayList_ver30_guideline"
        app:layout_constraintTop_toBottomOf="@id/inspectionArrayList_hor08_guideline"
        />

    <TextView
        android:id="@+id/inspectionArrayList_inspection_date"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:text="@string/sample_inspection_date_full"
        app:autoSizeTextType="uniform"
        android:textAlignment="textEnd"
        app:layout_constraintStart_toEndOf="@+id/inspectionArrayList_ver44_guideline"
        app:layout_constraintTop_toBottomOf="@+id/inspectionArrayList_hor08_guideline"
        app:layout_constraintEnd_toStartOf="@id/inspectionArrayList_ver63_guideline"/>


</androidx.constraintlayout.widget.ConstraintLayout>